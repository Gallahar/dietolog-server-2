{
	"version": 2,
	"installCommand": "npm install --legacy-peer-deps",
	"buildCommand": "npm run build",
	"builds": [
		{
			"src": "./dist/main.ts",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "./dist/main.ts",
			"methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
		}
	]
}
