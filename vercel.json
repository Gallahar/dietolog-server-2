{
	"version": 2,
	"installCommand": "npm install --legacy-peer-deps",
	"build": {
		"command": "npm install --legacy-peer-deps"
	},
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "dist/main.js",
			"methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
		}
	]
}
